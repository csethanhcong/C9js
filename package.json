{
  "name": "C9js",
  "version": "1.1.1-originally-released",
  "main": "dist/C9.min.js",
  "keywords": [
    "api",
    "js",
    "library",
    "map",
    "chart",
    "d3",
    "openlayer",
    "leaflet",
    "interactive",
    "data",
    "visualization",
    "visual",
    "image",
    "graph",
    "JavaScript",
    "datavis"
  ],
  "description": "A JavaScript library, used for interactive map-chart based on various types of data.",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "watch:test": "npm t -- -w",
    "test": "mocha test/index.test.js --compilers js:babel-register",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "dev": "webpack --output-filename C9.js --progress --colors --watch",
    "build": "npm-run-all --parallel build:*",
    "build:es6": "babel --out-dir dist src",
    "build:umd": "webpack --output-filename C9.js",
    "build:umd.min": "webpack --output-filename C9.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "d3": "4.2.6"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.6",
    "css-loader": "0.25.0",
    "cz-conventional-changelog": "1.2.0",
    "extract-text-webpack-plugin": "1.0.1",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "mocha": "3.1.0",
    "npm-run-all": "3.1.0",
    "nyc": "8.3.0",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5",
    "style-loader": "0.13.1",
    "webpack": "1.13.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csethanhcong/C9js.git"
  },
  "author": "Cong Pham<cse.thanhcong@gmail.com> (http://c9js.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csethanhcong/C9js/issues"
  },
  "homepage": "http://c9js.me/"
}
